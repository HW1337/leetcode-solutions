var WordDictionary = function() {
    this.words = new Set();
};

WordDictionary.prototype.addWord = function(word) {
    this.words.add(word);
};

WordDictionary.prototype.search = function(word) {
    const regex = new RegExp('^' + word.replace(/\./g, '.') + '$');
    for (const w of this.words) {
        if (regex.test(w)) {
            return true;
        }
    }
    return false;
};
